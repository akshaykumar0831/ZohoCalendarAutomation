<suite name="zephyr application Test automation suite1" verbose="2">
    <listeners>
        <listener class-name="com.zephyr.framework.Utils.Listener"/>
        <listener class-name="com.zephyr.framework.extentReportListener.ExtentReportListener"/>
    </listeners>

    <test name="zephyr Testcases - Suite 1">
        <classes>
            <class name="com.zephyr.testcases.Verify_Creating_NewEvent"/>
            <class name="com.zephyr.testcases.Verify_Searching_CreatedEvent"/>
            <class name="com.zephyr.testcases.Verify_UpdatingEvent_NewDate"/>
            <class name="com.zephyr.testcases.Verify_DeleteEvent"/>
            <class name="com.zephyr.testcases.Verify_ProfileUpdate"/>
        </classes>
    </test>
</suite>
